MICRO_SERVICE_ADDRESS="localhost:16567"
MONGO_URL="mongodb://localhost:27017"
MONGO_DB_NAME="messanger"
MONGO_COLLECTION_NAME="messages"
SERVICE_NAME="messages-service"
RABBIT_MQ_URL="amqp://guest:guest@localhost:5672/"
MONGO_ROOM_ID_VAR_NAME="room_id"

# StartConsumer
CONSUMER=""
AUTO_ASK=true
CONSUMER_EXCLUSIVE=false
NO_LOCAL=false
CONSUMER_NO_WAIT=false

# Publish
EXCHANGE="messages-gateway"
MANDATORY=false
IMMEDIATE=false

# Queue
NAME="messages"
DURABLE=false
DELETE_WHEN_UNUSED=false
QUEUE_EXCLUSIVE=true
QUEUE_NO_WAIT=false

# QoS
PREFETCH_COUNT=1
PREFETCH_SIZE=0
GLOBAL=false

# ExchangeDeclare
TYPE="fanout"
EXCHANGE_DURABLE=true
AUTO_DELETE=false
INTERNAL=false
EXCHANGE_NO_WAIT=false
